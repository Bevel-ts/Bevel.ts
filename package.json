  },
  "dependencies": {
    "@libsql/client": "^0.15.7",
    "@radix-ui/react-dialog": "^1.1.14",
    "@rjsf/core": "^6.0.0-beta.10",
    "@rjsf/validator-ajv8": "^6.0.0-beta.10",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "framer-motion": "^12.15.0",
    "hono": "^4.7.10",
    "prism-react-renderer": "^2.4.1",
